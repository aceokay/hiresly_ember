{{yield}}
<div class="container">
  <h1>Hello, {{developer.name}}</h1>
  <br>
  <h3>Your Information:</h3>
  <ul>
    <li>LinkedIn: </li>
    <li>GitHub: </li>
    <li>Portfolio: </li>
  </ul>
  <br>
  <h3>Tests Started</h3>
  {{first-test developer=developer problems=problems showTest=showTest startTest="startTest" submitAnswer="submitAnswer"}}
  <br>
  <h3>Tests Completed</h3>
  <ul>
    {{#each developer.completedTests as |test|}}
    <li>{{test.problem.title}}</li>
    {{/each}}
  </ul>
</div>
